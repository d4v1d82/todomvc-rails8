<li id="<%= dom_id todo %>">
  <turbo-frame id="<%= dom_id todo %>">
    <%= form_with(model: todo,  data: { controller: "form-toggle" }) do |form| %>
      <div class="view">
        <%= form.checkbox :completed, class: "toggle", data: { action: "form-toggle#submit" } %>
        <label><%=todo.title%> <%= link_to "Edit", edit_todo_path(todo) %></label>
      </div>
    <% end %>
  </turbo-frame>
</li>